{
      "label": "Import Demo Data",
      "version": "1.0.0",
      "description": "Import Demo Data",
      "nodes": [
            {
                  "name": "Clean Menu",
                  "engine": "xiang",
                  "query": {
                        "sql": {
                              "stmt": "delete from `xiang_menu`"
                        }
                  }
            },
            {
                  "name": "Clean Admin",
                  "engine": "xiang",
                  "query": {
                        "sql": {
                              "stmt": "truncate `xiang_user`"
                        }
                  }
            },
            {
                  "name": "Data",
                  "script": "data"
            },
            {
                  "name": "Menu",
                  "process": "xiang.helper.ArraySplit",
                  "args": [
                        "{{$res.Data.menu}}"
                  ]
            },
            {
                  "name": "Admin",
                  "process": "xiang.helper.ArraySplit",
                  "args": [
                        "{{$res.Data.admin}}"
                  ]
            },
            {
                  "name": "Insert",
                  "process": "models.xiang.menu.Insert",
                  "args": [
                        "{{$res.Menu.columns}}",
                        "{{$res.Menu.values}}"
                  ]
            },
            {
                  "name": "Insert Admin",
                  "process": "models.xiang.user.Insert",
                  "args": [
                        "{{$res.Admin.columns}}",
                        "{{$res.Admin.values}}"
                  ]
            }
      ],
      "output": {}
}